{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "parser": "babel-eslint"
  },
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "jquery": true,
    "es6": true
  },
  "extends": ["plugin:vue/essential", "plugin:prettier/recommended", "airbnb-base"],
  "plugins": ["vue", "prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "semi": false,
        "printWidth": 110
      }
    ],
    "function-paren-newline": "off",
    "implicit-arrow-linebreak": "off",
    "operator-linebreak": "off",
    "import/extensions": "off",
    "global-require": "off",
    "no-param-reassign": "off",
    "import/no-unresolved": "off",
    "no-await-in-loop": "off",
    "space-before-function-paren": "off",
    "no-underscore-dangle": "off",
    "no-restricted-syntax": "off",
    "object-curly-newline": "off",
    "guard-for-in": "off",
    "semi": "off",
    "indent": "off",
    "new-cap": "off",
    "max-len": "off",
    "func-names": "off",
    "arrow-parens": "off",
    "valid-jsdoc": "off",
    "eol-last": "off",
    "array-bracket-spacing": "off",
    "no-unused-vars": "off",
    "no-else-return": "off",
    "strict": "off",
    "linebreak-style": "off",
    "no-irregular-whitespace": [
      "error",
      {
        "skipComments": true
      }
    ],
    "comma-dangle": [
      "error",
      {
        "arrays": "only-multiline",
        "objects": "only-multiline"
      }
    ]
  },
  "globals": {
    "wx": true,
    "__wxConfig": true,
    "uni": true,
    "getApp": true,
    "getCurrentPages": true
  }
}
